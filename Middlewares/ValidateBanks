const { AddBankDTO } = require("../DTO/AddBankDTO");

function validateBanks(req, res, next) {
 try {
    addBankInfo  = new AddBankDTO(req.body);
    req.addBankData = addBankInfo;
    next();
 }
catch (err) {
  res.status(400).json({ message: err.message });
 }
}
module.exports = { validateBanks }